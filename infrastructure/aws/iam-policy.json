{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::yawbdata-raw",
        "arn:aws:s3:::yawbdata-processed"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::yawbdata-raw/*",
        "arn:aws:s3:::yawbdata-processed/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::yawbdata-processed/output",
        "arn:aws:s3:::yawbdata-processed/output/*"
      ]
    }
  ]
}
